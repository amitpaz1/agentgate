{
  "name": "@agentgate/server",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:generate:sqlite": "drizzle-kit generate --config=drizzle.sqlite.config.ts",
    "db:generate:postgres": "drizzle-kit generate --config=drizzle.postgres.config.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:sqlite": "drizzle-kit migrate --config=drizzle.sqlite.config.ts",
    "db:migrate:postgres": "drizzle-kit migrate --config=drizzle.postgres.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:studio:sqlite": "drizzle-kit studio --config=drizzle.sqlite.config.ts",
    "db:studio:postgres": "drizzle-kit studio --config=drizzle.postgres.config.ts",
    "bootstrap": "tsx src/bootstrap.ts"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@agentgate/core": "workspace:*",
    "@hono/node-server": "^1.13.0",
    "@hono/zod-validator": "^0.7.6",
    "better-sqlite3": "^11.6.0",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.38.0",
    "hono": "^4.6.0",
    "ioredis": "^5.9.2",
    "nanoid": "^5.0.0",
    "pino": "^10.3.0",
    "postgres": "^3.4.0",
    "safe-regex2": "^5.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.0",
    "@types/node": "^25.2.0",
    "@types/nodemailer": "^7.0.9",
    "drizzle-kit": "^0.30.0",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  }
}
